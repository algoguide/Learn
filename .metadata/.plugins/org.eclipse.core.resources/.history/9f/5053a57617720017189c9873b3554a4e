package sorts;

public class SortBubble {
	
	public static void main(String[] args){
		int[] intArray = {9,5,3,1,2,4,7,8,6};
		SortBubble a = new SortBubble();
		a.display(intArray);
		a.sort(intArray);
		a.swap(intArray[1], intArray[2]);
		
	}
	
	private void display(int[] arrayToDisplay){
		System.out.println("The length of the Integer Array is: " + arrayToDisplay.length);
		System.out.print("The Elements of the Integer Array are: ");
		for(int i=arrayToDisplay.length-1;i>=0;i--){
			if(i==0)
				System.out.print(arrayToDisplay[i]);
			else
				System.out.print(arrayToDisplay[i] + ",");
		}
	}
	
	private void sort(int[] arrayToSort){
		for(int i=arrayToSort.length-1;i>=0;i--){
			
		}
		
	}
	
	private void swap(int a, int b){
		System.out.println("The Integers passed are: " + a + " " + b);
	}

}
